<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_expense">

    <fragment
        android:id="@+id/navigation_expense"
        android:name="com.studentlifemanager.expense.ui.ExpenseFragment"
        android:label="@string/title_expense"
        tools:layout="@layout/fragment_myexpense" />

    <fragment
        android:id="@+id/navigation_notes"
        android:name="com.studentlifemanager.ui.pin.PinFragment"
        android:label="@string/title_pin"
        tools:layout="@layout/fragment_pin" />

    <fragment
        android:id="@+id/navigation_task"
        android:name="com.studentlifemanager.ui.task.TaskFragment"
        android:label="@string/title_task"
        tools:layout="@layout/fragment_task" />
</navigation>